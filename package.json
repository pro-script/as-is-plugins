{
  "name": "@pro-script/as-is-plugins",
  "version": "0.2.3",
  "description": "Plugins for @pro-script/as-is",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/numbers.browser.js",
      "module": "./dist/numbers.esm.mjs",
      "browser": "./dist/numbers.browser.js",
      "default": "./index.js"
    }
  },
  "engines": {
    "node": ">=14.18.3"
  },
  "directories": {},
  "scripts": {
    "test": "node tests/plugins.test.js",
    "build": "node build.config.js",
    "build:esm": "export NODE_ENV=esm && node build.config.js",
    "build:common": "export NODE_ENV=common && node build.config.js",
    "build:browser": "export NODE_ENV=browser && node build.config.js"
  },
  "keywords": [
    "is.zero",
    "is.zero",
    "is.even",
    "is.odd",
    "is.positive",
    "is.negative",
    "is.positiveInteger",
    "is.negativeInteger",
    "is.finite",
    "is.nan",
    "is.between",
    "is.greater",
    "is.less",
    "is.equalGreater",
    "is.equalLess",
    "is.max",
    "is.min",
    "is.multiple",
    "is.port",
    "is.safe",
    "is.precision",
    "is.digits",
    "is.ISBN10",
    "is.ISBN13",
    "is.EAN",
    "is.SSN",
    "is.VIN",
    "is.INN10",
    "is.INN12",
    "is.GLN",
    "is.IMEI",
    "is.NPI"
  ],
  "author": "Volodymyr Kotov (aka Kotowolod) <kotowolod@gmail.com> UKRAINE",
  "license": "MIT",
  "files": [
    "*.mjs",
    "/lib/*.*",
    "/tests/*.*",
    "/documentation/*.*",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/pro-script/as-is-plugins",
  "bugs": {
    "url": "https://github.com/pro-script/as-is-plugins/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pro-script/as-is-plugins.git"
  },
  "dependencies": {
    "@pro-script/as-is": "^1.2.3"
  }
}
