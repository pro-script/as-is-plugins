{
  "name": "@pro-script/as-is-plugins",
  "version": "0.3.92",
  "description": "Plugins for @pro-script/as-is",
  "main": "index.js",
  "type": "module",
  "./": {
    "import": "./index.js",
    "require": "./index.common.cjs"
  },
  "./numbers": {
    "require": "./dist/numbers.common.cjs",
    "import": "./dist/numbers.esm.mjs",
    "browser": "./dist/numbers.browser.js"
  },
  "./strings": {
    "require": "./dist/strings.common.cjs",
    "import": "./dist/strings.esm.mjs",
    "browser": "./dist/strings.browser.js"
  },
  "engines": {
    "node": ">=14.18.3"
  },
  "directories": {},
  "scripts": {
    "test": "node tests/plugins.test.js",
    "build:number": "export LIB='./numbers-validator.js' && node numbers.build.config.js",
    "build:strings": "export LIB='./strings-validator.js' && node strings.build.config.js"
  },
  "keywords": [
    "is zero",
    "is zero",
    "is even",
    "is odd",
    "is positive",
    "is negative",
    "is positiveInteger",
    "is negativeInteger",
    "is finite",
    "is nan",
    "is between",
