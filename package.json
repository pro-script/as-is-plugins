  "author": "Volodymyr Kotov (aka Kotowolod) <kotowolod@gmail.com> UKRAINE",
  "license": "MIT",
  "files": [
    "*.mjs",
    "/lib/*.*",
    "/tests/*.*",
    "/dist/*.*",
    "/documentation/*.*",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/pro-script/as-is-plugins",
  "bugs": {
    "url": "https://github.com/pro-script/as-is-plugins/issues"
  },
  "repository": {
    "type": "git",
